CREATE TABLE Habilidade (
    id SERIAL PRIMARY KEY,
    nome VARCHAR(50) NOT NULL,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);

CREATE TABLE Sub_Habilidade (
    id SERIAL PRIMARY KEY,
    nome VARCHAR(50) NOT NULL,
    id_habilidade INT NOT NULL, -- Define o tipo da coluna
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    CONSTRAINT fk_habilidade FOREIGN KEY (id_habilidade) REFERENCES Habilidade (id)
);

