CREATE TABLE Usuario_Sub_Habilidade (
    id SERIAL PRIMARY KEY,
    id_users INT NOT NULL,
    id_sub_habilidade INT NOT NULL,
    descricao TEXT,
    valor DECIMAL(10, 2),
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    CONSTRAINT fk_user FOREIGN KEY (id_users) REFERENCES users(id),
    CONSTRAINT fk_sub_habilidade FOREIGN KEY (id_sub_habilidade) REFERENCES Sub_Habilidade(id)
);
